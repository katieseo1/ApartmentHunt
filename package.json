{
  "name": "apartment-hunt",
  "version": "0.0.1",
  "description": "A React template",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "dev": "run-p watch serve",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "build": "run-s build:*",
    "clean": "rimraf build",
    "serve": "node server.js",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cpr index.html build/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "run-s clean:js mkdir:js _build:js",
    "_build:js": "webpack",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "clean:css": "rimraf build/css",
    "mkdir:css": "run-s mkdir",
    "build:css": "run-s clean:css mkdir:css _build:css",
    "_build:css": "cpr css build/css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf build/assets",
    "build:assets": "run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "cpr assets build/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
  },
  "author": "Katie",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chokidar-cli": "^1.2.0",
    "cpr": "^2.0.0",
    "enzyme": "^2.7.1",
    "enzyme-redux": "^0.1.6",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.1",
    "npm-run-all": "^3.0.0",
    "react-addons-test-utils": "^15.4.2",
    "redux-test-utils": "^0.1.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1",
    "webpack-dotenv-plugin": "^2.0.0"
  },
  "dependencies": {
    "body-parser": "^1.16.0",
    "chai-http": "^3.0.0",
    "config": "^1.25.1",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.4.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.1",
    "isomorphic-fetch": "^2.2.1",
    "js-alert": "^1.0.4",
    "jsdom": "^9.11.0",
    "nock": "^9.0.6",
    "node-googleplaces": "^0.1.2",
    "react": "^15.4.2",
    "react-activity-indicator": "^0.7.7",
    "react-dom": "^15.4.2",
    "react-geosuggest": "^2.1.0",
    "react-google-autocomplete": "^1.0.12",
    "react-places-autocomplete": "^4.1.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.2",
    "redux-thunk": "^2.1.0",
    "reverse-geocoding": "^2.0.0",
    "whatwg-fetch": "^2.0.2"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "apartment",
    "react"
  ]
}
